{
  "presets": [
    "next/babel", "es2015"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd"
      }
    ],
    ["transform-class-properties"],
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "styles": ["./components", "./styles"]
        },
        "cwd": "babelrc"
      }
    ],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ]
}
